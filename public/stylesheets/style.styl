a
  color: #00B7FF
.fb-panel
  background: #3B5998
  padding: 0
  a
    color: #fff
    display: block
    padding: 20px
    border: 1px solid #0F6E94
  a:hover
    border: 1px solid #fff
.top-bar.expanded
  z-index: 2
.logo
  width: 40px
  margin-right: 15px
#users
  list-style: none
  li
    min-height: 32px
    a
      line-height: 32px
      padding-left: 20px
      display: block
    a.isonline
      background: transparent url(../images/online.png) 7px 50% no-repeat
    a:hover
      color: black
      background-color: #E0E4EE
    img.profile
      margin-right: 10px
